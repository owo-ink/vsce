{
	"name": "owo",
	"displayName": "owo",
	"description": "",
	"version": "0.0.5",
	"publisher": "puge",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"repository": {
    "type": "git",
    "url": ""
  },
	"main": "./extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.helloWorld",
			"title": "Hello World",
			"breakpoints": [
				{
					"language": "owo"
				}
			],
			"languages": [
				{
					"id": "owo",
					"aliases": [
						"owo",
						"OWO"
					],
					"extensions": [
						".owo"
					],
					"configuration": "./languages/owo-language-configuration.json"
				}
			],
			"grammars": [
				{
					"language": "owo",
					"scopeName": "source.owo",
					"path": "./syntaxes/vue-generated.json",
					"embeddedLanguages": {
						"text.html.basic": "html",
						"text.html.vue-html": "vue-html",
						"text.jade.slm": "slm",
						"text.pug": "jade",
						"text.haml": "haml",
						"text.html.liquid": "liquid",
						"source.css": "css",
						"source.css.scss": "scss",
						"source.css.less": "less",
						"source.css.postcss": "vue-postcss",
						"source.sass": "sass",
						"source.stylus": "stylus",
						"source.js": "javascript",
						"source.ts": "typescript",
						"source.coffee": "coffeescript",
						"text.html.markdown": "md",
						"source.yaml": "yaml",
						"source.json": "json",
						"source.php": "php",
						"source.graphql": "graphql"
					}
				}
			]
		}]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
