{
	"name": "owo",
	"displayName": "owo",
	"publisher": "PUGE",
	"description": "针对owo语言的高亮提示,自动补全",
	"version": "0.0.5",
	"repository": {
		"type": "git",
		"url": ""
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:owo"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
		],
		"languages": [
			{
				"id": "owo",
				"aliases": [
					"owo",
					"OWO"
				],
				"extensions": [
					".owo"
				],
				"configuration": "./src/languages/owo-language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "owo",
				"scopeName": "source.owo",
				"path": "./syntaxes/vue-generated.json",
				"embeddedLanguages": {
					"text.html.basic": "html",
					"text.html.vue-html": "vue-html",
					"text.jade.slm": "slm",
					"text.pug": "jade",
					"text.haml": "haml",
					"text.html.liquid": "liquid",
					"source.css": "css",
					"source.css.scss": "scss",
					"source.css.less": "less",
					"source.css.postcss": "vue-postcss",
					"source.sass": "sass",
					"source.stylus": "stylus",
					"source.js": "javascript",
					"source.ts": "typescript",
					"source.coffee": "coffeescript",
					"text.html.markdown": "md",
					"source.yaml": "yaml",
					"source.json": "json",
					"source.php": "php",
					"source.graphql": "graphql"
				}
			},
			{
				"path": "./syntaxes/pug/directives.tmLanguage.json",
				"scopeName": "vue.pug.directives",
				"injectTo": [
					"source.owo"
				]
			},
			{
				"path": "./syntaxes/pug/interpolations.tmLanguage.json",
				"scopeName": "vue.pug.interpolations",
				"injectTo": [
					"source.owo"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/lodash": "^4.14.149",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-languageserver": "^5.2.1",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"vscode-css-languageservice": "^4.0.2",
		"vscode-languageserver-types": "^3.14.0"
	}
}
